{
  "root":true,
  "parser": "@typescript-eslint/parser",
  "plugins": [
    "@typescript-eslint",
    "prettier"
  ],
  "extends": [
    "next/core-web-vitals",
    "eslint:recommended",
    "plugin:@typescript-eslint/eslint-recommended",
    "plugin:@typescript-eslint/recommended",
    "google",
    "prettier"],
  "rules": {
    "no-console": 1,       // Means warning
    "prettier/prettier": 2,
    "valid-jsdoc": 0,
    "require-jsdoc": 0
  }
}
